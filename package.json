{
  "name": "wikipathways-api-client",
  "version": "1.0.10",
  "bin": "./bin/cli.js",
  "description": "JS client for the WikiPathways.org webservices API",
  "main": "wikipathways-api-client.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "compile": "tsc --project tsconfig.json",
    "start:mockserver": "./node_modules/mockserver/bin/mockserver.js -p 4522 -m './test/mockserver-data'",
    "test": "echo 'add working tests'",
    "watch": "npm-run-all -p start:mockserver watch:*",
    "watch:client": "npm-watch",
    "watch:webpack": "npm run start:webpack"
  },
  "browserify": {
    "transform": [
      "brfs",
      "deglobalify"
    ]
  },
  "browser": {
    "atob": false,
    "btoa": false,
    "commander": false,
    "inquirer": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/wikipathways-api-client-js"
  },
  "keywords": [
    "wikipathways",
    "biology",
    "pathway"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/wikipathways-api-client-js/issues"
  },
  "homepage": "https://github.com/wikipathways/wikipathways-api-client-js",
  "dependencies": {
    "@types/lodash": "^4.14.62",
    "JSONStream": "^1.3.1",
    "atob": "^1.1.2",
    "cheerio": "^0.19.0",
    "colors": "^1.1.2",
    "hyperquest": "^1.3.0",
    "inquirer": "^0.11.0",
    "lodash": "^4.17.4",
    "ndjson": "^1.5.0",
    "rx-extra": "^2.0.4",
    "sax": "^1.1.1",
    "urijs": "^1.15.x",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "commander": "~2.3.0",
    "deglobalify": "^0.2.0",
    "gulp": "^3.9.0",
    "mkdirp": "^0.5.1",
    "mockserver": "^1.5.2",
    "shebang-loader": "0.0.1",
    "ts-loader": "^2.0.3",
    "typescript": "^2.2.2",
    "webpack": "^2.4.1"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [],
      "excludeDir": [
        "./dist",
        "./node_modules"
      ]
    }
  }
}
